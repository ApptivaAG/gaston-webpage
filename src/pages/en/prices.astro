---
import { SEO } from 'astro-seo'

import Main from '@layouts/Main.astro'
import Container from '@components/Container.astro'

const description =
  'Choose the right offer for your needs. Gaston the digital menu allows guests to order and pay quickly and easily. It doesn\'t matter if the guests are in their establishment, on the road or at home. Gaston supports ordering in your business, take-away and delivery.'
const title =
  'Offer - Gaston - Digital Menu - Serve more guests faster with fewer staff.'

---

<head>
  <SEO {title} {description} />
</head>

<Main>
  <Container>
    <h1>Prices</h1>
  </Container>
</Main>